sub logical_cx
increment i to n_log
    increment j to n_log
        if logical_index1 == i
            if logical_index2 == j
                increment k to n_phys_per_log
                    qubit_array.[n_phys_per_log*logical_index1 + k-1] -> qubit_ctrl
                    qubit_array.[n_phys_per_log*logical_index2 + k-1] -> qubit_tgt
                    cnot[qubit_ctrl, qubit_tgt]
                next
            end
        end

        end
    next
next
