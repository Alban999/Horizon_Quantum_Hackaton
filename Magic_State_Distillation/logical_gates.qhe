sub logical_x
    increment i to n_log
        if logical_index1 == i
            increment j to n_phys_per_log
                qubit_array[n_phys_per_log*logical_index1 + j-1] -> qubit_reg.[j]
                x[qubit_reg.[j]]
            next
        end
    next
endsub

